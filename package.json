{
  "name": "manual",
  "private": true,
  "scripts": {
    "livereload": "livereload --exclusions node_modules/ --exts 'css,js,twig,theme,apng,avif,gif,jpg,jpeg,jfif,pjpeg,pjp,png,svg,webp'",
    "sass": "sass --source-map --error-css --style=compressed --load-path=node_modules scss/base.scss:dist/css/base.css",
    "js": "./node_modules/.bin/esbuild js/*.js --outdir=dist/js --minify"
  },
  "devDependencies": {
    "livereload": "^0.9.3"
  },
  "dependencies": {
    "esbuild": "^0.20.2",
    "normalize.css": "^8.0.1",
    "sass": "^1.72.0"
  }
}
